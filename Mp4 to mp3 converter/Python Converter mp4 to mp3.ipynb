    "from moviepy.editor import *\n",
    "\n",
    "mp4_file = \"video.mp4\"\n",
    "\n",
    "mp3_file = \"audio.mp3\"\n",
    "\n",
    "videoClip = VideoFileClip(mp4_file)\n",
    "\n",
    "audioclip = videoClip.audio\n",
    "\n",
    "audioclip.write_audiofile(mp3_file)\n",
    "\n",
    "audioclip.close()\n",
    "\n",
    "videoClip.close()"
   ]
  }
